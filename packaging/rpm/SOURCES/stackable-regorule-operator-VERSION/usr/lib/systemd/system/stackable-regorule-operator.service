[Unit]
Description=Stackable RegoRule Operator
Before=
After=network.target
[Service]
User=root
ExecStart=/opt/stackable/stackable-regorule-operator-server/stackable-regorule-operator
Restart=on-abort
StandardOutput=journal
StandardError=journal
Environment="REGORULE_OPERATOR_LOG=info"
[Install]
WantedBy=multi-user.target
